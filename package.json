{
  "name": "caloriecam",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.17.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "prettier -w .",
